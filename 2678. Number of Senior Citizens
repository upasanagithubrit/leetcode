class Solution {
public:
    int countSeniors(vector<string>& details) {
        int count=0;
        for(int i=0;i<details.size();i++)
            {
            string check=details[i];
            if((check[11]-'0')>=6 && (check[12]-'0'>0))
                {
                count++;
                }
              
           else  if((check[11]-'0')>6 && (check[12]-'0' >=0))
                {
                count++;
                }
            
            }
        return count;
        
    }
};
